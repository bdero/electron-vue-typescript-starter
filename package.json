{
  "name": "electron-vue-typescript-starter",
  "version": "1.0.0",
  "description": "An Electron starter that supports VueJS + TypeScript.",
  "main": "src/main.ts",
  "dependencies": {
    "electron-compile": "^6.4.3",
    "electron-squirrel-startup": "^1.0.0",
    "vue": "^2.4.4",
    "vueify": "^9.4.1",
    "electron-devtools-installer": "^2.2.0"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.0.2",
    "electron-forge": "^4.0.2",
    "electron": "9.1.0",
    "electron-prebuilt-compile": "1.7.6",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2"
  },
  "scripts": {
    "start": "node_modules/.bin/electron .",
    "lint": "node_modules/.bin/tslint --project tsconfig.json --type-check --force",
    "make": "node_modules/.bin/electron-forge make"
  },
  "author": "Brandon DeRosier",
  "license": "MIT",
  "config": {
    "forge": "./forge.config.js"
  }
